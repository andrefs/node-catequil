{
  "name": "catequil",
  "version": "0.0.1",
  "description": "A chat app written in Express + MongoDB + React + Redux",
  "main": "index.js",
  "scripts": {
    "dev:initdb": "mongofixtures catequil_dev server/database/fixtures_dev.js",
    "dev:static": "gulp",
    "dev:server": "nodemon server/server-entry.js",
    "prod:initdb": "mongofixtures catequil server/database/fixtures_prod.js",
    "prod:static": "gulp -- build",
    "prod:server": "NODE_ENV=production nodemon server/server-entry.js",
    "test": "npm-run-all test:dropdb test:initdb test:run",
    "test:run": "NODE_ENV=test mocha -S --compilers js:babel-core/register --require ./test/server/test_helper.js  --recursive ./test/server",
    "test:initdb": "mongofixtures catequil_test server/database/fixtures_test.js",
    "test:dropdb": "mongo catequil_test --eval 'db.dropDatabase();'",
    "test:react": "NODE_ENV=test mocha -S --require ./test/react/test_helper.js  --recursive ./test/react",
    "gulp": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andrefs/.git"
  },
  "keywords": [
    "chat",
    "react",
    "redux",
    "express",
    "mongodb",
    "mongo",
    "gulp",
    "catequil"
  ],
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "author": "Andr√© Santos",
  "license": "ISC",
  "dependencies": {
    "async": "2.1.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.17.1",
    "bootstrap": "3.3.7",
    "debug": "2.6.1",
    "express": "4.15.2",
    "express-favicon": "2.0.0",
    "express-jwt": "5.1.0",
    "gulp-cssimport": "5.0.0",
    "hbs": "4.0.1",
    "immutable": "3.8.1",
    "jquery": "3.1.1",
    "jwt-simple": "0.5.1",
    "mongoose": "4.8.6",
    "mongoose-paginate": "5.0.3",
    "morgan": "1.8.1",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-local": "1.0.0",
    "pow-mongodb-fixtures": "0.14.0",
    "react": "15.4.2",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-actions": "2.0.1",
    "redux-auth-wrapper": "1.0.0",
    "redux-immutable": "3.1.0",
    "redux-logger": "2.8.2",
    "redux-thunk": "2.2.0",
    "socket.io": "1.7.3",
    "socket.io-client": "1.7.3",
    "socketio-jwt": "4.5.0",
    "source-sans-pro": "2.0.10"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "6.23.0",
    "babelify": "7.3.0",
    "browserify": "14.1.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "enzyme": "2.7.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.1",
    "gulp-concat": "2.6.1",
    "gulp-cssmin": "0.1.7",
    "gulp-if": "2.0.2",
    "gulp-less": "3.3.0",
    "gulp-plumber": "1.1.0",
    "gulp-sourcemaps": "2.4.1",
    "gulp-uglify": "2.1.0",
    "gulp-util": "3.0.8",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "react-addons-test-utils": "15.4.2",
    "supertest": "3.0.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.9.0"
  }
}
