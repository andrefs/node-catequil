{
  "name": "catequil",
  "version": "0.0.1",
  "description": "A chat app written in Express + MongoDB + React + Redux",
  "main": "index.js",
  "scripts": {
    "dev:initdb": "mongofixtures catequil_dev server/database/fixtures_dev.js",
    "dev:static": "gulp",
    "dev:server": "nodemon server/server-entry.js",
    "prod:initdb": "mongofixtures catequil server/database/fixtures_prod.js",
    "prod:static": "gulp -- build",
    "prod:server": "NODE_ENV=production nodemon server/server-entry.js",
    "test": "npm-run-all test:initdb test:run",
    "test:run": "NODE_ENV=test mocha -S --compilers js:babel-core/register --require ./test/test_helper.js  --recursive",
    "test:initdb": "mongofixtures catequil_test server/database/fixtures_test.js",
    "gulp": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andrefs/.git"
  },
  "keywords": [
    "chat",
    "react",
    "redux",
    "express",
    "mongodb",
    "mongo",
    "gulp",
    "catequil"
  ],
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "author": "Andr√© Santos",
  "license": "ISC",
  "dependencies": {
    "async": "2.0.0-rc.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.7.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.15.0",
    "bootstrap": "3.3.6",
    "debug": "2.2.0",
    "express": "4.13.4",
    "express-favicon": "1.0.1",
    "express-jwt": "3.3.0",
    "gulp-cssimport": "3.0.2",
    "hbs": "4.0.0",
    "immutable": "3.8.1",
    "jquery": "2.2.3",
    "jwt-simple": "0.5.0",
    "mongoose": "4.4.12",
    "mongoose-paginate": "5.0.0",
    "morgan": "1.7.0",
    "nodemon": "1.9.1",
    "npm-run-all": "1.7.0",
    "passport": "0.3.2",
    "passport-facebook": "2.1.0",
    "passport-local": "1.0.0",
    "pow-mongodb-fixtures": "0.13.0",
    "react": "15.0.1",
    "react-bootstrap": "0.29.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-redux": "4.0.2",
    "redux": "3.5.1",
    "redux-actions": "0.9.1",
    "redux-auth-wrapper": "0.3.0",
    "redux-immutable": "3.0.6",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "socket.io": "1.4.5",
    "socket.io-client": "1.4.5",
    "socketio-jwt": "4.3.4",
    "source-sans-pro": "2.0.10"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "6.6.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "chai-http": "2.0.1",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "3.1.0",
    "gulp-concat": "2.6.0",
    "gulp-cssmin": "0.1.7",
    "gulp-if": "2.0.0",
    "gulp-less": "3.0.5",
    "gulp-plumber": "1.1.0",
    "gulp-sourcemaps": "2.0.0-alpha",
    "gulp-uglify": "1.5.3",
    "gulp-util": "3.0.7",
    "mocha": "2.4.5",
    "nodemon": "1.9.1",
    "supertest": "1.2.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0"
  }
}
